{
   "manifest_version": 3,
   "name": "Linkfeed",
   "version": "1.0",
   "description": "Click the Linkfeed button to share a link to your page. Your friends can follow your page in a browser, or with an RSS reader, no signup or email required.",
   "permissions": [
       "storage",
       "activeTab"
   ],
   "action": {
       "default_popup": "popup.html",
       "default_icon": {
         "512": "512.png",
         "256": "256.png",
         "128": "128.png"

       }
   },
   "icons": {
       "512": "512.png",
       "256": "256.png",
       "128": "128.png"
   },
   "browser_specific_settings": {
    "gecko": {
      "id": "{0e25664b-ffbc-4ef8-ad7b-a367e2984688}",
      "strict_min_version": "42.0"
    },
    "safari": {
      "strict_min_version": "14"
    }
  }  
  }
  